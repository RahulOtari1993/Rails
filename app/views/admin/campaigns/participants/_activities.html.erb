<div class="tab-pane active" id="Activity_Feed" aria-labelledby="Insights-tab" role="tabpanel">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Action</th>
        <th scope="col">Points</th>
        <th scope="col">Date & time</th>
      </tr>
    </thead>
    <tbody>
      <% @participant.participant_actions.each do |participant_action| %>
        <tr>
          <td><%= title_details(participant_action) %></td>
          <td><%= participant_action.points == 0 ? "N/A" : participant_action.points %></td>
          <td><span><%= participant_action.created_at.strftime("%m/%d/%Y %H:%M %p") %></span></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>