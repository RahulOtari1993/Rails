      text-align: left;
      padding-left: 20px;
    }

    .video-watch-modal .challenge-complete a.btn-share {
      color: #fefefe;
      background-color: #BB0000;
      text-align: center;
      width: inherit;
      margin: 0px 0px 10px 0px;
    }

    .video-watch-modal .challenge-complete h1 {
      vertical-align: top;
      margin-top: 20px;
      font-size: 38px;
      color: #666666;
    }

    .video-watch-modal .challenge-complete p {
      font-size: 26px;
      color: #666666;
    }

    .video-watch-modal .video-watch-title {
      color: #bb0000;
      font-size: 38px;
      font-weight: 600;
      margin-bottom: 20px;
    }

    @media (max-width: 700px) {
      .video-watch-modal .video-box-left {
        width: 64%;
      }

      .video-watch-modal .video-box-right {
        width: 100%;
        text-align: center;
        padding-left: 0px;
      }
    }

    .meter {
      height: 4px;
      position: relative;
      background: #e9e9e9;
      -moz-border-radius: 25px;
      -webkit-border-radius: 25px;
      border-radius: 0;
      padding: 0;
      box-shadow: inset 0 -1px 1px rgba(255, 255, 255, 0.3);
      color: #ffffff;
      margin-top: 6px;
    }

    .meter > span {
      vertical-align: top;
      display: block;
      height: 100%;
      background-color: #bb0000;
      box-shadow: inset 0 2px 9px rgba(255, 255, 255, 0.3),
      inset 0 -2px 6px rgba(0, 0, 0, 0.4);
      position: relative;
      overflow: hidden;
    }

    .challenge-complete {
      text-align: center;
    }

    .challenge-complete h1 {
      color: #bb0000;
      font-size: 42px;
    }

    .challenge-complete p {
      color: #bb0000;
      font-weight: 400;
      font-size: 30px;
    }

    .challenge-complete a.btn-share {
      color: #fefefe;
      background-color: #BB0000;
      margin: 10px 170px;
    }

    /* Fixes smooshed View Available Rewards button on mobile Challenge Complete modal  */
    @media (max-width: 600px) {
      .challenge-complete a.btn-share {
        margin: 10px 10px;
      }
    }

    .challenge-complete a.btn-has-icon {
      display: block;
    }

    .fb-detail-block {
      border: solid 1px lightgrey;
    }

    .fb-detail-inner-block {
      padding: 0px 15px;
    }

    .fb-detail-inner-block h1 {
      font-size: 28px;
      color: black;
      font-weight: normal;
    }

    p.fb-content {
      margin-top: 5px;
    }

    .fb-comment-avi {
      padding: 20px 0;
    }

    .fb-comment-avi textarea {
      display: inline-block;
      max-width: 80%;
      width: 80%;
      vertical-align: top;
      resize: none;
      font-size: 15px;
      cursor: pointer;
      border: 1px solid lightgray;
      padding: 5px;
      height: 86px;
    }

    .fb-comment-avi img {
      display: inline-block;
    }

    .loader {
      border: 16px solid #f3f3f3;
      border-radius: 50%;
      border-top: 16px solid #bb0000;
      width: 40px;
      height: 40px;
      -webkit-animation: spin 2s linear infinite; /* Safari */
      animation: spin 2s linear infinite;
    }

    /* Safari */
    @-webkit-keyframes spin {
      0% {
        -webkit-transform: rotate(0deg);
      }
      100% {
        -webkit-transform: rotate(360deg);
      }
    }

    @keyframes spin {
      0% {
        transform: rotate(0deg);
      }
      100% {
        transform: rotate(360deg);
      }
    }

    .open-link span {
      transition: all 0.05s ease-in-out;
      width: 30px;
      height: 3px;
      display: block;
      background: #1F232A;
      border-radius: 3px;
      margin: 0 0 6px 0;
    }

    .open-link span:last-child {
      margin: 0;
    }

    .open-link.active span:nth-of-type(1) {
      -webkit-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      transform: rotate(45deg);
      margin: 10px 0 0 0;
    }

    .open-link.active span:nth-of-type(2) {
      opacity: 0;
    }

    .open-link.active span:nth-of-type(3) {
      -webkit-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
      transform: rotate(-45deg);
      margin: -11px 0 0 0;
    }
  </style>
</head>

